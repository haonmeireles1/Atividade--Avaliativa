#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main() {
    float chico = 1.5, ze = 1.1;
    int ano = 0;

    while(chico >= ze){
        
        chico += 0.02;
        
        ze += 0.03;
        
        ano++;
        printf("Chico: %.2f\tZe: %.2f\tAno: %d\n", chico, ze, ano);
    }
    printf("É necessário %d para que Zé seja maior que Chico\n\n", ano);
    
    
}
